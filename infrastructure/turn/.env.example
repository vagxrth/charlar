# Shared with backend â€” MUST match apps/server TURN_SECRET
TURN_SECRET=

# Realm used in TURN authentication
TURN_REALM=turn.example.com

# Public IP of the TURN server (required for NAT traversal)
TURN_EXTERNAL_IP=

# IP to bind to (0.0.0.0 = all interfaces)
TURN_LISTENING_IP=0.0.0.0

# Path to TLS cert and key inside the container (for TURNS on port 5349)
TURN_TLS_CERT=/etc/turn/certs/turn.crt
TURN_TLS_KEY=/etc/turn/certs/turn.key

# UDP relay port range
TURN_MIN_PORT=49152
TURN_MAX_PORT=65535
